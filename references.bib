@Article{Bauer04,
  Title                    = {Novel Superconductivity and Magnetism in CePt3Si},
  Author                   = {Bauer, E.
and Hilscher, G.
and Michor, H.
and Sieberer, M.
and Scheidt, E.W.
and Gribanov, A.
and Seropegin, Yu.
and Rogl, P.
and Song, W.Y.
and Park, J.-G.
and Adroja, D.T.
and Amato, A.
and Nicklas, M.
and Sparn, G.
and Yogi, M.
and Kitaoka, Y.},
  Journal                  = {Czechoslovak Journal of Physics},
  Year                     = {2004},

  Month                    = {Dec},
  Number                   = {4},
  Pages                    = {401--406},
  Volume                   = {54},

  Abstract                 = {CePt3Si exhibits antiferromagnetic order at TN ≈ 2.2 K and superconductivity (SC) at Tc ≈ 0.75 K. Large values of H{\textasciiacutex}c2 ≈ −8.5 T/K and Hc2(0) ≈ 5 T indicate Cooper pairs formed out of heavy quasiparticles. The mass enhancement originates from Kondo interaction with a characteristic temperature TK ≈ 8 K. NMR and $\mu$SR measurements evidence coexistence of SC and long-range magnetic order on a microscopic scale. Moreover, CePt3Si is the first heavy-fermion SC without an inversion symmetry. This gives rise to a novel type of the NMR relaxation rate 1/T1 never reported before for other heavy-fermion superconductors. Studies of Si/Ge substitution allow us to establish a phase diagram.},
  Day                      = {01},
  Doi                      = {10.1007/s10582-004-0107-2},
  ISSN                     = {1572-9486},
  Url                      = {https://doi.org/10.1007/s10582-004-0107-2}
}

