@Article{Paglione10,
  author   = {Paglione, Johnpierre and Greene, Richard L.},
  title    = {High-temperature superconductivity in iron-based materials},
  journal  = {Nature Physics},
  year     = {2010},
  volume   = {6},
  number   = {9},
  pages    = {645-658},
  month    = {9},
  issn     = {1745-2481},
  abstract = {The surprising discovery of superconductivity in layered iron-based materials, with transition temperatures climbing as high as 55{\thinspace}K, has led to thousands of publications on this subject over the past two years. Although there is general consensus on the unconventional nature of the Cooper pairing state of these systems, several central questions remain --- including the role of magnetism, the nature of chemical and structural tuning, and the resultant pairing symmetry --- and the search for universal properties and principles continues. Here we review the progress of research on iron-based superconducting materials, highlighting the main experimental benchmarks that have been reached so far and the important questions that remain to be conclusively answered.},
  day      = {01},
  doi      = {10.1038/nphys1759},
  url      = {https://doi.org/10.1038/nphys1759},
}

